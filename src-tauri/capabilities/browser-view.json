{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "browser-view",
  "description": "Capabilities for the Browser window with toolbar and content webviews",
  "windows": ["browser-toolbar"],
  "webviews": ["browser-toolbar", "browser-content"],
  "remote": {
    "urls": ["https://*", "http://*"]
  },
  "permissions": [
    "core:default",
    "core:window:allow-close",
    "core:window:allow-show",
    "core:window:allow-set-focus"
  ]
}
